<script>

    console.log("{{pageName}}")

    activeLink()

    function activeLink()
    {
        $(".list-group-item").removeClass("active");
        $("#li-{{currentPage}}").addClass("active");
        $('#action-loader-wrapper').css('display', 'none');

        if("{{currentPage}}" == "appsetup" || "{{currentPage}}" == "configuration" || "{{currentPage}}" == "globalappsetup" || "{{currentPage}}" == "globalsetting")
        {
            $('#fixedBottomAdminMenu').css('display', 'none');
            $('#main-menu-enabled').css('display', 'none');
            $('#footer-menu-enabled').css('display', 'block');
        }
        else
        {

            $('#fixedBottomAdminMenu').css('display', 'block');
            $('#main-menu-enabled').css('display', 'block');
            $('#footer-menu-enabled').css('display', 'none');
        }
    }

    function mainSidebarRedirection(path) 
    {
        if(enableInquestPermissionToggle)
        {
            if(path === 'testcase' || path === 'testcycle' || path === 'testplan') 
            {
                attach_file_list=[];
                $('#action-loader-wrapper').css('display', 'flex');
                $.ajax({
                    "type": "GET",
                    "url": '/folder/testFlag?project_id={{projectId}}&acct_id={{userAccountId}}&user={{userAccountId}}&test_type=' + path + '&domain_id=' + domain_id,
                    "headers": {
                        'Authorization': "JWT "+user_token,
                    },
                    "dataType": "json",
